{
    "name": "walirazzaq/laravel-model-attribute-events",
    "description": "This is my package laravel-model-attribute-events",
    "keywords": [
        "walirazzaq",
        "laravel",
        "laravel-model-attribute-events"
    ],
    "homepage": "https://github.com/walirazzaq/laravel-model-attribute-events",
    "license": "MIT",
    "authors": [
        {
            "name": "Wali Razzaq",
            "email": "m.wali.razzaq@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.10",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Walirazzaq\\AttributeEvents\\": "src",
            "Walirazzaq\\AttributeEvents\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Walirazzaq\\AttributeEvents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Walirazzaq\\AttributeEvents\\LaravelModelAttributeEventsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
